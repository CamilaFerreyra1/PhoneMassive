CREATE FUNCTION NotCharOpc(IN ref REFERENCE, IN refError REFERENCE) RETURNS BOOLEAN
BEGIN
	DECLARE err REFERENCE TO refError.Code;
	IF (ref IS NOT NULL) AND (ref <> '') THEN
		IF ((LENGTH(TRANSLATE(UPPER(CAST(ref AS CHARACTER)) ,'ABCDEFGHIJKLMNÃ‘OPQRSTUVWXYZ ', '')) <> 0)) THEN
			CREATE LASTCHILD OF refError AS err NAME 'Code';
			SET err = 'GSS-422-002';
			RETURN TRUE;
		END IF;
	END IF;
	RETURN FALSE;
END;