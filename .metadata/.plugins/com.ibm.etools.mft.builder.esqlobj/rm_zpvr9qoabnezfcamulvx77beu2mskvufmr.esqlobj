CREATE FUNCTION GetValue(IN url CHARACTER, IN ref CHARACTER) RETURNS CHARACTER
BEGIN
	IF CONTAINS(url, ref) THEN
		DECLARE aux CHARACTER SUBSTRING(url AFTER ref);
		DECLARE aux2 CHARACTER SUBSTRING(aux BEFORE ' HTTP/1.1');
		IF(CONTAINS(aux2,'/')) THEN
		RETURN (SUBSTRING(aux2 BEFORE '/'));
	END IF;
	RETURN aux2;
END IF;
RETURN NULL;
END;