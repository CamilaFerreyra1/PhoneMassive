CREATE FUNCTION NotDateTimeOpc(IN dateF REFERENCE, IN refError REFERENCE) RETURNS BOOLEAN
BEGIN
	IF(dateF IS NOT NULL) THEN
	DECLARE F CHARACTER TransFromTimestampToDotNetDate(CAST(dateF AS CHARACTER));
	SET dateF = CAST(REPLACE(F, 'T', ' ') AS TIMESTAMP DEFAULT CAST('1900-01-10 00:00:00' AS TIMESTAMP));
	IF (dateF = CAST('1900-01-10 00:00:00' AS TIMESTAMP)) THEN
		CREATE LASTCHILD OF refError NAME 'Code' VALUE 'GSS-422-002';
		RETURN TRUE;
	END IF;
	RETURN FALSE;
END IF;
RETURN FALSE;
END;