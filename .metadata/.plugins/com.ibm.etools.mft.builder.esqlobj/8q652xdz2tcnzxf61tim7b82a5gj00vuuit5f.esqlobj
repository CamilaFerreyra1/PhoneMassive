CREATE FUNCTION NotStringOpc(IN ref REFERENCE, IN refError REFERENCE) RETURNS BOOLEAN
BEGIN
	IF (ref IS NOT NULL) AND (ref <> '') THEN
		DECLARE CONTINUE HANDLER FOR SQLSTATE LIKE '%' BEGIN END;
		DECLARE aux INTEGER;
		SET aux = CAST(ref AS INTEGER);
		SET ref = ref - 1;
		IF UPPER(ref) = 'NULL' OR (aux <> CAST(ref AS INTEGER)) THEN
			CREATE LASTCHILD OF refError NAME 'Code' VALUE 'GSS-422-002';
			RETURN TRUE;
		END IF;
	END IF;
	RETURN FALSE;
END;