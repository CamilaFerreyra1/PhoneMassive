CREATE FUNCTION NotDateTypeOpc(IN paramDate REFERENCE, IN refError REFERENCE) RETURNS BOOLEAN
BEGIN
	IF paramDate IS NOT NULL THEN
		DECLARE dateF DATE CAST(paramDate AS DATE DEFAULT CAST('1900-01-10' AS DATE));
		IF dateF = '1900-01-10' THEN
			CREATE LASTCHILD OF refError NAME 'Code' VALUE 'GSS-422-002';
			RETURN TRUE;
		END IF;
	END IF;
	RETURN FALSE;
END;